            </columns>
        </TableView>
        <Button layoutX="442.0" layoutY="26.0" mnemonicParsing="false" onAction="#proceed" prefHeight="33.0" prefWidth="140.0" text="Proceed" />
        <Label fx:id="msg" layoutX="163.0" layoutY="26.0" prefHeight="33.0" prefWidth="256.0" />
        <Button layoutX="243.0" layoutY="370.0" mnemonicParsing="false" onAction="#newStore" prefHeight="33.0" prefWidth="97.0" text="New Store" />
